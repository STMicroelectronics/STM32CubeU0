#include "region_defs.h"

LR_CODE BL2_CODE_START {
    ER_BOOT_CODE (BL2_CODE_START) {
        *.o (RESET +First)
        .ANY (+RO)
    }

    ER_CODE (PERSO_START) FIXED (PERSO_SIZE) {
    *(.bootutil_key_cnt, +FIRST)
    *(.bootutil_key)
    *(.bootutil_enc_key, +LAST)
    }

    ER_PROVISION_4 +0  {
        keys.o
    }

    ER_DATA (BL2_DATA_START) (BL2_DATA_SIZE-BL2_MSP_STACK_SIZE-BL2_HEAP_SIZE) {
        .ANY (+ZI +RW)
    }

    /* MSP */
    ARM_LIB_STACK (0x20008000) EMPTY BL2_MSP_STACK_SIZE {
    }

    ARM_LIB_HEAP +0 ALIGN 8 EMPTY BL2_HEAP_SIZE {
    }

    NOINIT_DATA +0 UNINIT {
      *(.bss.NoInit)
    }
}
